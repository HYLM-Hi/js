
.head{
  display: flex;
  justify-content:center;
  padding: 5px 10px 10px 10px;
   font-weight: 900;
}
.navbar{  
  flex: none;  
  display: flex;  
  background: #fff;  
}  
.item{  
  position: relative;  
  flex: auto;  
  text-align: center;  
  line-height: 80rpx;  
}  
.active{  
  color: #00b26a;  
}  
.active:after{  
  content: "";  
  display: block;  
  position: absolute;  
  bottom: 0;  
  left: 0;  
  right: 0;  
  height: 3px;  
  background: #00b26a;  
}
.list {
  display: block;
  width: 100%;
  padding: 10px;
  border-top: 1px solid #e6e6e6;
}  
.list1 {
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 10px;
  border-top: 1px solid #e6e6e6;
} 
.list2{
  display: block;
  white-space: nowrap;
} 
.list3 {
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 10px;
  border-top: 1px solid #e6e6e6;
  border-bottom: 1px solid #e6e6e6;
} 
